{
  "name": "rabbit/base",
  "description": "rabbit base",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "albert",
      "email": "63851587@qq.com"
    }
  ],
  "require": {
    "php": ">=8.0.0",
    "ext-swoole": ">=4.7.0",
    "psr/log": "^1.0",
    "php-di/php-di": "^6.2",
    "ext-mbstring": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "swoole/ide-helper": "@dev",
    "phpunit/phpunit": "*"
  },
  "suggest": {
    "ext-dom": "*",
    "ext-simplexml": "*"
  },
  "autoload": {
    "files": [
      "src/functions.php"
    ],
    "psr-4": {
      "Rabbit\\Base\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rabbit\\Base\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "./vendor/bin/phpunit"
    ]
  }
}
